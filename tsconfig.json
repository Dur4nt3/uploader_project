{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ES2023",
        "types": [],
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "noUncheckedIndexedAccess": true,
        "strict": true,
        "jsx": "react-jsx",
        "verbatimModuleSyntax": true,
        "isolatedModules": true,
        "noUncheckedSideEffectImports": true,
        "moduleDetection": "force",
        "skipLibCheck": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "ignoreDeprecations": "6.0"
    }
}
